##  Session alias statements 
unalias cd
alias cds cds61
alias ls 'ls --color'
alias la 'ls --color -a'
alias ll 'ls --color -l'
alias lla 'ls --color -la'
alias llr 'ls --color -lrt'
alias llar 'ls --color -lart'
alias du 'du -sbc * | sort -n' 
alias df df -k
alias em 'emacs \!* &'
alias where 'echo $HOST'
alias term 'konsole &'
alias ff 'firefox &'
alias tb 'thunderbird &'
alias cp 'cp -p'
alias cl clear
alias mv 'mv -i'
alias pu pushd
alias po popd
alias more less
alias vnc15 'vncserver -name kde -depth 24 -geometry 1024x718'
alias vnc19 'vncserver -name kde -depth 24 -geometry 1280x974'
alias vnc19lin 'vncserver -name kde -depth 24 -geometry 1280x960'
alias vnc20 'vncserver -name kde -depth 24 -geometry 1600x1150'
alias mvu /cad/adi/cadlib/models/programs/mvu/mvu
alias cls 'clear'
alias gdb 'gdb --quiet'
alias tmux 'tmux -2'

## this is for managing dotfiles
alias got 'git --git-dir=$HOME/.dotfiles --work-tree=$HOME'
alias dot 'git --git-dir=$HOME/.dotfiles --work-tree=$HOME'
alias gitt 'git --git-dir=$HOME/.dotfiles --work-tree=$HOME'


