##  Emacs (-e) or Vim (-v) command key bindings
#bindkey -v
bindkey -e
#bindkey ^[. set-mark-command

##  Autocompletion (lists ambiguous completions, up to 10 lines)
unset autolist 
# set autolist = ambiguous
set listmaxrows = 10
